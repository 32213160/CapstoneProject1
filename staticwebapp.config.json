{
  "routes": [
    {
      "route": "/api/auth/status",
      "rewrite": "https://torytestsv.kro.kr/api/auth/status",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/auth/me",
      "rewrite": "https://torytestsv.kro.kr/api/auth/me",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/auth/login",
      "rewrite": "https://torytestsv.kro.kr/api/auth/login",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/auth/register",
      "rewrite": "https://torytestsv.kro.kr/api/auth/register",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/auth/logout",
      "rewrite": "https://torytestsv.kro.kr/api/auth/logout",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/auth/setlevel",
      "rewrite": "https://torytestsv.kro.kr/api/auth/setlevel",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/chats-of-user/my-sessions",
      "rewrite": "https://torytestsv.kro.kr/api/chats-of-user/my-sessions",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/chats-of-user/session/*",
      "rewrite": "https://torytestsv.kro.kr/api/chats-of-user/session/{sessionId}",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/upload",
      "rewrite": "https://torytestsv.kro.kr/api/upload",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/chat",
      "rewrite": "https://torytestsv.kro.kr/api/chat",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/api/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
    "Access-Control-Allow-Headers": "Content-Type, Authorization",
    "Access-Control-Allow-Credentials": "true"
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
